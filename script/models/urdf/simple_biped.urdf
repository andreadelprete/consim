<?xml version="1.0"?>
<robot name="planar_biped">
  <link name="base">
      <inertial>
         <origin rpy="0 0 0" xyz="0 0 0"/>
         <mass value="2.0"/>
         <inertia ixx=".01613" ixy="0.0" ixz="0.0" iyy=".01613" iyz="0.0" izz=".01613"/>
      </inertial>

      <visual>
          <geometry>
              <mesh filename="package://meshes/box.stl" scale=".22 .22 .22"/>
              <!--<box size="0.22 0.22 0.22"/>-->
          </geometry>
          <material name="white">
              <color rgba="1 1 1 1"/>
          </material>
      </visual>
       <collision>
          <geometry>
              <mesh filename="package://meshes/box.stl" scale=".22 .22 .22"/>
          </geometry>
      </collision>
  </link>

